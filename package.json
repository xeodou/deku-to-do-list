  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xeodou/deku-to-do-list/issues"
  },
  "homepage": "https://github.com/xeodou/deku-to-do-list",
  "devDependencies": {
    "babelify": "^6.1.0",
    "browserify": "^10.1.3",
    "deku": "^0.2.17",
    "gh-pages": "^0.3.0"
  }
}
