{
  "name": "deku-todo",
  "version": "1.0.0",
  "description": "A to do example using deku library",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "build": "./node_modules/.bin/browserify -t babelify index.js > build.js",
